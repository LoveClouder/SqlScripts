##2013全年有消费客户数###
select
	count(distinct a.client_id) as clientcount
from (select
				client_id,
				sum(f_consume) as cost
			from weibo_report
			where statdate between '2013-01-01' and '2013-12-31'
			group by client_id
			having sum(f_consume) &gt; 0) a
where not exists (select 1 from (select	#截止2013-12-31连续3个月以内有消费的客户数，用not exists 过滤
									co.client_id,
									sum(point_cost) as cost
								from client_order co
								where create_time between '2013-10-01' and '2013-12-31 23:59:59' and code = 'WEIBO-CPC'
								group by co.client_id
								having sum(point_cost) &gt; 0) c where a.client_id = c.client_id)
							
###2013年TOP10代理商的消耗及实际收入贡献###
select
	wr.agent_name,
	sum(wr.cost)*100 as cost,
	sum(rc.cost) as cost_rc,
	sum(rc.`donate_cost`) as donate_cost
from (select
				client_id,
				agent_name,
				sum(f_consume) as cost
			from weibo_report a
			where a.statdate between '2013-01-01' and '2013-12-31'
			group by client_id,agent_name having sum(f_consume) &gt; 0 ) wr
inner join (select
						co.client_id,
						sum(point_cost) as cost,
						sum(donate_cost) as donate_cost
					from client_order co
					where co.code = 'WEIBO-CPC' and co.create_time between '2013-01-01' and '2013-12-31 23:59:59'
					group by co.client_id
					) rc on wr.client_id = rc.client_id 
group by wr.agent_name


	





			
