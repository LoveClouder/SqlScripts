##新开客户且当月有消费##
select
	c.create_month,
	count(distinct c.client_id) as newclientcount
from (select distinct
				DATE_FORMAT(create_time, '%Y-%m') as create_month,
				client_id
			from client c
			where create_time between '2013-11-01' and '2013-12-31 23:59:59') c
inner join (select distinct
						DATE_FORMAT(create_time, '%Y-%m') as charge_month,
						client_id
					from client_order
					where create_time between '2013-11-01' and '2013-12-31 23:59:59'
						and code = 'WEIBO-CPC') cg on c.client_id = cg.client_id and c.create_month = cg.charge_month
group by c.create_month

