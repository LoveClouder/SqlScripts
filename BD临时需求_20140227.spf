select
	b.statmonth,
	clientcount_hascost,
	b.totalcost/clientcount_hascost as arpu
from (select 
				statmonth,
				count(distinct client_id) as clientcount_hascost,
				sum(cost) as totalcost,
				DATEDIFF(DATE_ADD(CONCAT(statmonth,"-01"), interval 1 month), CONCAT(statmonth,"-01")) as daycount
			from (select
				DATE_FORMAT(wr.statdate, '%Y-%m') as statmonth,
				agent_name,
				client_id,
				case
					when screen_name = '' then client_name
					else screen_name
				end as client_name,
				sum(pv) as pv,
				sum(iv) as iv,
				sum(f_consume) as cost
			from weibo_report wr
			where statdate between '2013-10-01' and '2013-12-31' 
			group by
				DATE_FORMAT(wr.statdate, '%Y-%m'),
				agent_name,
				client_id,
				case
					when screen_name = '' then client_name
					else screen_name
				end
			having sum(f_consume) &gt; 0 ) a
			group by statmonth
			) b
UNION ALL
select
	b.statmonth,
	clientcount_hascost,
	b.totalcost/clientcount_hascost as arpu
from (select 
				statmonth,
				count(distinct client_id) as clientcount_hascost,
				sum(cost) as totalcost,
				92 as daycount
			from (select
				'2013Q4' as statmonth,
				agent_name,
				client_id,
				case
					when screen_name = '' then client_name
					else screen_name
				end as client_name,
				sum(pv) as pv,
				sum(iv) as iv,
				sum(f_consume) as cost
			from weibo_report wr
			where statdate between '2013-10-01' and '2013-12-31' 
			group by
				agent_name,
				client_id,
				case
					when screen_name = '' then client_name
					else screen_name
				end
			having sum(f_consume) &gt; 0 ) a
			group by statmonth
			) b
			
